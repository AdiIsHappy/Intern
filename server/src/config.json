{
  "port": 4000,
  "redis": {
    "host": "localhost",
    "port": 6379
  },
  "db": {
    "root": "./tmp/"
  },
  "bull": {
    "maxParallelJobs": 1
  },
  "analysis": {
    "reportUpdatePeriod": "week",
    "fetchDataPeriodLengthinWeeks": 28,
    "dataPerJob": {
      "mergeRequests": 5,
